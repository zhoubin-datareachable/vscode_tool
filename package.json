{
  "name": "react-tool",
  "displayName": "react工具",
  "description": "react工具",
  "keywords": [
    "react工具"
  ],
  "publisher": "zhoubin",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.59.1"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onCommand:react_tool.addComponents",
    "onCommand:react_tool.addModuleComponents",
    "onView:commonTool",
    "onView:docs",
    "onView:collection"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react_tool.addComponents",
        "title": "新建一般组件"
      },
      {
        "command": "react_tool.addModuleComponents",
        "title": "新建组件库组件"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "react_tool.addComponents",
          "group": "navigation"
        },
        {
          "command": "react_tool.addModuleComponents",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "properties": {
        "author_name": {
          "type": "string",
          "default": "admin",
          "description": "请输入用户名"
        },
        "collection": {
          "type": "array",
          "default": [
            {
              "title": "Web 技术周刊",
              "url": "https://www.yuque.com/awesome/fe_weekly"
            }
          ],
          "description": "请添加网址"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tool",
          "title": "工具箱",
          "icon": "images/tool.svg"
        }
      ]
    },
    "views": {
      "tool": [
        {
          "id": "commonTool",
          "name": "常用工具"
        },
        {
          "id": "docs",
          "name": "文档查询"
        },
        {
          "id": "collection",
          "name": "个人收藏"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.59.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "ts-loader": "^9.2.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/zhoubin-datareachable/vscode-tool/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhoubin-datareachable/vscode-tool"
  },
  "homepage": "https://github.com/zhoubin-datareachable/vscode-tool/README.md"
}
